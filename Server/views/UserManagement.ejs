<!DOCTYPE HTML>
<html lang="en">
    <%- include('./Layouts/Header.ejs'); -%>
<body>
<!-- main -->
<div class="center-container">
	<!--header-->
	<div class="header-w3l">
		<h1>Change Information Of User Form</h1>
	</div>
	<!--//header-->
	<div class="main-content-agile">
		<div class="sub-main-w3">	
			<div class="wthree-pro">
				<h2>User Management</h2>
			</div>
			<form action="#" method="post">
				<div class="pom-agile">
					<input placeholder="User Name" name="Name" class="user" type="text">
					<span class="icon1"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
				<div class="pom-agile">
					<input  placeholder="Password" name="Password" class="pass" type="password">
					<span class="icon2"><i class="fa fa-unlock" aria-hidden="true"></i></span>
				</div>
                <div class="pom-agile">
					<input  placeholder="Enter Again Password" name="Password" class="pass" type="password">
					<span class="icon2"><i class="fa fa-unlock" aria-hidden="true"></i></span>
				</div>
                <div class="pom-agile">
					<input placeholder="Choose Aavata to change" name="Name" class="user" type="file">
					<span class="icon1"><i class="fa fa-user" aria-hidden="true"></i></span>
				</div>
				<div class="sub-w3l">
					<div class="right-w3l">
						<input type="submit" value="Change">
					</div>
				</div>
			</form>
		</div>
	</div>
	<!--//main-->
	<%- include('./Layouts/Header.ejs'); -%>
</div>
</body>
</html>